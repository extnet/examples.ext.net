@page "{handler?}"
@model Ext.Net.Examples.Pages.samples.layout.vboxlayout.rows.IndexModel
@{
    Layout = null;
}
<!DOCTYPE html>
<html>
    <head>
        <title>
            VBoxLayout Rows - Ext.NET Examples
        </title>
        <link href="/resources/css/examples.css" rel="stylesheet" />
    </head>
    <body>
        <ext-viewport layout="VBox">
            <layout>
                <ext-vboxLayout align="Stretch" />
            </layout>
            <items>
                <ext-panel title="Height = 25%" flex="1" />
                <ext-panel title="Height = 180px" height="180" />
                <ext-panel title="Height = 75%" flex="3" />
            </items>
        </ext-viewport>
    </body>
</html>
